{
    "name": "No Ad C:",
    "description": "This is adremover",
    "version": "0.4",
    "manifest_version": 2,
    "icons": {
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "browser_action": {
        "default_popup": "/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
